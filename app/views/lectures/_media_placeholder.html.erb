
  <table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Created At</th>
      <th>Media</th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% media.each do |element| %>
      <tr>
        <td><%= element.name %></td>
        <td><%= element.created_at %></td>

        <td> <% if element.attributes['url'] %>
            <%= image_tag element.url %>
        <% end %> </td>

        <td> <%= link_to 'Delete', '#', :class => 'btn btn-mini btn-danger' %> </td>
      </tr>
    <% end %>
  </tbody>
</table>

<hr>


